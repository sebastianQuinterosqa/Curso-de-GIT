package clase2;
     //4-Pedir el ingreso de un dato, e indicar si es menor o mayor a 0

import java.util.Scanner;

public class ejercicio4 {
	//INICIO
	public static void main(String args[]) {
		
	//Int num = 0;
		int num = 0;
		
	//TECLADO
		Scanner input = new Scanner(System.in);
		
	//MOSTRAR “Ingrese un numero”;
		System.out.println("ingrese un numero");
		
	//GUARDAR num;
		num = input.nextInt();
	
	//if ( num > 0 ) {
		if (num > 0) {
			
		//	MOSTRAR “El numero es mayor a 0”;
			System.out.println("el numero es mayor a 0");
			
	
		}else if (num < 0) {
			System.out.println("el numero es menor a 0");
			
		}
	//}
	//else {
		else {
			
		
	//	MOSTRAR “El numero es menor a 0”;
			System.out.println("el numero es igual a 0");
		}
	//}
	//FIN
	
	}

}
